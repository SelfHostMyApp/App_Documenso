# Documenso Configuration
DOCUMENSO_VERSION=latest
DOCUMENSO_PORT=8084
DOCUMENSO_SUBDOMAIN=documenso
ROOT_DOMAIN=example.com

# Authentication & Security
DOCUMENSO_NEXTAUTH_SECRET=change_this_nextauth_secret_to_something_secure
DOCUMENSO_ENCRYPTION_KEY=change_this_encryption_key_32_chars_long
DOCUMENSO_ENCRYPTION_SECONDARY_KEY=change_this_secondary_encryption_key

# Database Configuration (PostgreSQL)
DOCUMENSO_DB_USER=documenso
DOCUMENSO_DB_NAME=documenso
DOCUMENSO_PASSWORD=documenso_secure_password
POSTGRES_HOST=postgres
POSTGRES_PORT=5432
DOCUMENSO_DATABASE_URL=postgres://documenso:documenso_secure_password@postgres:5432/documenso
DOCUMENSO_DIRECT_DATABASE_URL=postgres://documenso:documenso_secure_password@postgres:5432/documenso

# Application URLs
DOCUMENSO_WEBAPP_URL=https://documenso.example.com
DOCUMENSO_INTERNAL_WEBAPP_URL=http://localhost:8084

# File Upload Configuration
DOCUMENSO_UPLOAD_TRANSPORT=database

# Email Configuration (SMTP)
DOCUMENSO_SMTP_TRANSPORT=smtp
DOCUMENSO_SMTP_HOST=smtp.example.com
DOCUMENSO_SMTP_PORT=587
DOCUMENSO_SMTP_USERNAME=documenso@example.com
DOCUMENSO_SMTP_PASSWORD=smtp_password
DOCUMENSO_SMTP_SECURE=true
DOCUMENSO_SMTP_FROM_NAME=Documenso
DOCUMENSO_SMTP_FROM_ADDRESS=documenso@example.com

# Google OAuth (optional)
# DOCUMENSO_GOOGLE_CLIENT_ID=your_google_client_id
# DOCUMENSO_GOOGLE_CLIENT_SECRET=your_google_client_secret

# Other Settings
DOCUMENSO_DOCUMENT_SIZE_UPLOAD_LIMIT=50MB
DOCUMENSO_DISABLE_SIGNUP=false
DOCUMENSO_SIGNING_LOCAL_FILE_PATH=/opt/documenso/cert.p12
# DOCUMENSO_SIGNING_PASSPHRASE=your_certificate_passphrase